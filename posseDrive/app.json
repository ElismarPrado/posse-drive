{
  "expo": {
    "name": "Posse Drive",
    "slug": "posseDrive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.insite.possedrive"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.insite.possedrive",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "chave google"
        }
      }
    }
  }
}
